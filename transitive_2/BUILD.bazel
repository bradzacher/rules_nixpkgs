load("@nixpkgs_packages//:defs.bzl", "get_pkg")

genrule(
    name = "generated",
    srcs = [get_pkg("transitive", "2.1.4", "tool", "package")],
    outs = ["generated.out"],
    cmd = """\
echo 'generated by transitive 2.1.4 with' > $(OUTS)
cat $(SRCS) >> $(OUTS)
""",
    visibility = ["//visibility:public"],
)
